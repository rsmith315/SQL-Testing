/**Creating a table and inserting data
**/

/**List Information
table name: students
columns: first_name, last_name
Amy Bridge
Susan Red
Burt Kent
Austin Powers
**/

CREATE TABLE students (id INTEGER PRIMARY KEY, first_name TEXT, last_name TEXT);

